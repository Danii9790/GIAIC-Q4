{
  "version": 2,
  "functions": {
    "webhook.py": {
      "runtime": "python3.9"
    }
  },
  "routes": [
    {
      "src": "/set-appointment",
      "dest": "webhook.py"
    },
    {
      "src": "/webhook",
      "dest": "webhook.py"
    }
  ],
  "env": {
    "TWILIO_ACC_SID": "@twilio_acc_sid",
    "TWILIO_AUTH_TOKEN": "@twilio_auth_token",
    "GEMINI_API_KEY": "@gemini_api_key"
  }
}